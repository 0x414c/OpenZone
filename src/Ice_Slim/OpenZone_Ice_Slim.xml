<theme name="OpenZone_Ice_Slim">
  <source size="32" gridsize="32" margin="2" spacing="4">
    <image file="hotspots.png" use="hotspot"/>
    <image file="0.png" use="0"/>
    <image file="1.png" use="1"/>
    <image file="2.png" use="2"/>
    <image file="3.png" use="3"/>
    <image file="4.png" use="4"/>
    <image file="5.png" use="5"/>
    <image file="6.png" use="6"/>
    <image file="7.png" use="7"/>
    <image file="8.png" use="8"/>
    <image file="9.png" use="9"/>
    <image file="10.png" use="10"/>
    <image file="11.png" use="11"/>
    <image file="12.png" use="12"/>
    <image file="13.png" use="13"/>
  </source>

  <layout>
    <row>
      <cursor name="left_ptr"/>
      <cursor name="whats_this"/>
      <cursor name="copy"/>
      <cursor name="move"/>
      <cursor name="link"/>
      <cursor name="forbidden"/>
      <cursor name="left_ptr_watch">
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
      </cursor>
      <cursor name="wait">
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
        <frame delay="50"/>
      </cursor>
    </row>
    <row>
      <cursor name="right_ptr"/>
      <cursor name="pointing_hand"/>
      <cursor name="openhand"/>
      <cursor name="closedhand"/>
      <cursor name="ibeam"/>
      <cursor name="vertical-text"/>
      <cursor name="cross"/>
      <cursor name="size_all"/>
    </row>
    <row>
      <cursor name="size_hor"/>
      <cursor name="size_ver"/>
      <cursor name="size_fdiag"/>
      <cursor name="size_bdiag"/>
      <cursor name="up_arrow"/>
      <cursor name="down_arrow"/>
      <cursor name="right_arrow"/>
      <cursor name="left_arrow"/>
    </row>
    <row>
      <cursor name="pencil"/>
      <cursor name="color-picker"/>
      <cursor name="zoom-in"/>
      <cursor name="zoom-out"/>
      <cursor name="cell"/>
      <cursor name="dot"/>
      <cursor name="dotbox"/>
      <cursor name="X_cursor"/>
    </row>
    <row>
      <cursor name="pirate"/>
    </row>
  </layout>

  <!-- complete to full Qt specification -->
  <alias name="split_h" target="size_hor"/>
  <alias name="split_v" target="size_ver"/>

  <!-- complete to full freedesktop.org specification -->
  <alias name="default" target="left_ptr"/>
  <alias name="text" target="ibeam"/>
  <alias name="pointer" target="pointing_hand"/>
  <alias name="help" target="whats_this"/>
  <alias name="progress" target="left_ptr_watch"/>
  <alias name="alias" target="link"/>
  <alias name="no-drop" target="forbidden"/>
  <alias name="not-allowed" target="forbidden"/>
  <alias name="all-scroll" target="size_all"/>
  <alias name="row-resize" target="size_hor"/>
  <alias name="col-resize" target="size_ver"/>
  <alias name="e-resize" target="size_hor"/>
  <alias name="ne-resize" target="size_bdiag"/>
  <alias name="nw-resize" target="size_fdiag"/>
  <alias name="n-resize" target="size_ver"/>
  <alias name="se-resize" target="size_fdiag"/>
  <alias name="sw-resize" target="size_bdiag"/>
  <alias name="s-resize" target="size_ver"/>
  <alias name="w-resize" target="size_hor"/>
  <alias name="crosshair" target="cross"/>
  <alias name="ew-resize" target="size_hor"/>
  <alias name="ns-resize" target="size_ver"/>
  <alias name="nesw-resize" target="size_bdiag"/>
  <alias name="nwse-resize" target="size_fdiag"/>

  <!-- add some standard X11 names -->
  <alias name="arrow" target="right_ptr"/>
